{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": {
          "jsx": "react"
        }
      }
    }
  },
  "dependencies": {
    "color": "3.1.2",
    "expo": "34.0.3",
    "jest": "24.9.0",
    "lodash": "4.17.15",
    "expo-font": "6.0.1",
    "native-base": "2.13.4",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.0.tar.gz",
    "react-native-web": "0.11.5"
  },
  "devDependencies": {
    "@babel/core": "*",
    "@babel/preset-env": "*",
    "@babel/preset-typescript": "7.7.7",
    "@types/jest": "24.0.25",
    "@types/react": "16.9.17",
    "@types/react-native": "0.57.65",
    "@types/react-test-renderer": "16.9.1",
    "babel-jest": "*",
    "babel-preset-expo": "6.0.0",
    "jest": "*",
    "jest-expo": "34.0.1",
    "react-test-renderer": "16.12.0",
    "ts-jest": "24.3.0",
    "typescript": "3.5.3"
  },
  "private": true
}
